<% @wineries.each do |w| %>
<div class="row">
     <div class="col s12 m3">
       <div class="card blue-grey darken-1">
         <div class="card-content white-text">
           <span class="card-title"><%= w.name %></span>
           <p><%= w.winery_pitch %></p>
         </div>
         <div class="card-action">
           <%= link_to("More Details", winery_path(w) ) %>
           <!-- <a href="http://www.google.com">Website</a> -->
           <!-- <a href="#">This is a link</a> -->
         </div>
       </div>
     </div>
    <% end %>
   </div>
<%= link_to "New Winery", new_winery_path %>
